{
    "semi": false,
    "singleQuote": false,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "es5",
    "printWidth": 120,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "plugins": ["prettier-plugin-tailwindcss"]
}
